# details on the px submission tool
px.submission.tool.name=${name}
px.submission.tool.version=${version}

# the default size of the submission tool
px.submission.tool.width = 800
px.submission.tool.height = 750

# url for retrieving pride user details
px.user.login.url=https://www.proteomexchange.org/archive-submission-ws/user/login

# url for retrieving pride Project Files details
px.submission.ws.base.url=https://www.proteomexchange.org/archive-submission-ws/
px.project.files.url=resubmission/files/{accession}

# url for retrieving px ftp upload details
px.upload.detail.url = https://www.proteomexchange.org/archive-submission-ws/submission/upload/{method}
px.reupload.detail.url = https://www.proteomexchange.org/archive-submission-ws/submission/reupload/{method}/{ticketId}
px.upload.verify.url = https://www.proteomexchange.org/archive-submission-ws/submission/fileListAndSize

# url for completing px submission
px.submission.complete.url = https://www.proteomexchange.org/archive-submission-ws/submission/submit

# url for proteomexchange
px.web.url = https://www.proteomexchange.org/

# url for proteomexchange guideline
px.submission.guideline.web.url = https://www.proteomexchange.org/submission/index.html

# url for pride new account
pride.new.account.url = https://www.ebi.ac.uk/pride/register

# Checksum
checksum.filename = checksum.txt
px.submission.complete.url = https://www.proteomexchange.org/archive-submission-ws/submission/submit
px.resubmission.complete.url = https://www.proteomexchange.org/archive-submission-ws/resubmission/resubmit

# urls for px submission tool
# GitHub releases for automated updates
px.submission.tool.update.url = https://api.github.com/repos/PRIDE-Archive/px-submission-tool/releases/latest
px.submission.tool.download.url = https://github.com/PRIDE-Archive/px-submission-tool/releases/latest/download/px-submission-tool-${version}.zip
px.submission.tool.url = https://www.proteomexchange.org/submission/index.html
px.submission.summary.file.format.url = https://www.proteomexchange.org/docs/Submission_Summary_File_Format.pdf

# url for resubmission
px.submission.detail.url = https://www.proteomexchange.org/archive-submission-ws/resubmission/projects

# aspera client binary (using ascp)
aspera.client.mac.binary = aspera/bin/mac-intel/ascp
aspera.client.linux32.binary = aspera/bin/linux-32/ascp
aspera.client.linux64.binary = aspera/bin/linux-64/ascp
aspera.client.windows32.binary = aspera/bin/windows-32/ascp.exe
aspera.client.windows64.binary = aspera/bin/windows-64/ascp.exe


# EBI aspera server details, not necessary for upload functionality
aspera.EBI.host = fasp.ebi.ac.uk
aspera.EBI.user = prd_ascp
aspera.EBI.password = path/to/keyFile?

# aspera transfer parameters (optimized for huge files)
# aspera.xfer.tcpPort = 22
aspera.xfer.tcpPort = 33001
aspera.xfer.udpPort = 33001

# Transfer rates optimized for huge files (GB/TB datasets)
# Target: ~1 Gbps (950 Mbps) - good balance of speed vs stability
aspera.xfer.targetRateKbps = 950000
# Minimum: 1 Mbps - ensures transfer continues even on slow networks
aspera.xfer.minimumRateKbps = 1000

# Fair policy for multiple concurrent transfers
aspera.xfer.policy = fair

# Huge file optimizations
aspera.xfer.cookie = PRIDE-Aspera-1-Cookie
aspera.xfer.token = PRIDE-Aspera-1-Token
aspera.xfer.preCalculateJobSize = false
# Create remote directories as needed
aspera.xfer.createPath = true

# Additional huge file optimizations (handled by ASCP4 command line)
# ASCP4 automatically uses --sparse-file, --chunk-size=134217728, --compression=lz4
# These are built into the ascp4 binary and don't need SDK configuration

# Aspera timeout settings (in milliseconds)
aspera.timeout.transfer = 600000
aspera.timeout.progress = 120000
aspera.timeout.monitoring = 60000
aspera.timeout.enableMonitoring = false

# Report subsystem parameters
libpxreport.keyfile.path = keyfiles/pride-submission-reports-e963f29e8d2a.p12
libpxreport.client.service.name = libpxreport
libpxreport.account.id = libpxreport@pride-submission-reports.iam.gserviceaccount.com
libpxreport.spreadsheet.title = px-submission-tool_userFeedback
libpxreport.spreadsheet.worksheet.title = mainSheet